<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "/assets/application-92cfd497e6f3f85e12d829b69eec4698.css", :media => "all" %>
  <%= javascript_include_tag "/assets/application-9ec4d88decc78fa59a5b08d306feffb9.js" %>

	<%= yield :javascript_includes %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container-fluid">
  <div class="row-fluid">
    <div class="span5 well" id="content"><!-- Content -->
      <section>
				<% flash.each do |key, value| %>
				  <div class="alert alert-<%= key %>"><%= value %></div>
				<% end %>
        <%= yield %>
      </section>
    </div><!-- Content -->
	<%= render 'layouts/sidebar' %>
  </div>
  <div class="row-fluid">
	<%= render 'layouts/footer' %>
  </div>
  <!-- <div style="margin-top:10px;"><%= debug(params) if Rails.env.development? %></div> -->
</div>
</body>
</html>
